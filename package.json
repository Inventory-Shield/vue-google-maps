{
  "name": "vue3-google-maps",
  "version": "0.13.0",
  "description": "This is a google map component for Vue.js, updated for Vue 3 compatibility",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run clean && npm run build:tsc && npm run build:webpack",
    "clean": "rm -r dist",
    "build:tsc": "tsc -p tsconfig.prod.json && echo {} > dist/.babelrc && node ./build-copy.js",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress",
    "build:examples": "npm run build:examples:before && npm run build:examples:webpack",
    "build:examples:before": "npm run build && shx cp dist/vue-google-maps.js examples",
    "build:examples:webpack": "cd examples && cross-env NODE_ENV=production webpack",
    "lab:tests": "lab -T test/test-setup/babel-transform.js -l -S test",
    "lint": "eslint --ext .vue,.js src && eslint --ext .vue,.html,.js test",
    "lint:fix": "eslint --fix --ext .vue,.js src && eslint --ext .vue,.html,.js test",
    "test": "npm run lab:tests && npm run lint",
    "deploy": "npm run build:examples && gh-pages -d examples",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Inventory-Shield/vue-google-maps.git"
  },
  "bugs": {
    "url": "https://github.com/Inventory-Shield/vue-google-maps/issues"
  },
  "homepage": "https://github.com/Inventory-Shield/vue-google-maps#readme",
  "dependencies": {
    "@googlemaps/markerclustererplus": "^1.1.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@hapi/lab": "^20.4.0",
    "@types/googlemaps": "^3.43.3",
    "@vue/compiler-sfc": "^3.0.7",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-vue": "^2.0.2",
    "consolidate": "^0.15.1",
    "cross-env": "^6.0.3",
    "css-loader": "^5.1.3",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^7.7.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^2.1.1",
    "json-loader": "^0.5.7",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "puppeteer": "^1.20.0",
    "raw-loader": "^4.0.2",
    "shx": "^0.3.2",
    "style-loader": "^2.0.0",
    "template-html-loader": "^1.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "vue": "^3.0.7",
    "vue-loader": "^16.1.2",
    "vue-router": "^3.1.3",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.26.0",
    "webpack-cli": "^4.5.0",
    "yaml-loader": "^0.5.0"
  },
  "author": "Daniel Sim, Guillaume Leclerc",
  "license": "MIT"
}
